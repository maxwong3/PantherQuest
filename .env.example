# Copy this file to `.env` and fill in real secrets. Do NOT commit `.env`.

# Postgres DB credentials
POSTGRES_USER=postgres
POSTGRES_PASSWORD=example
POSTGRES_DB=pantherquest

# DATABASE_URL used by backend (format: postgres://user:pass@host:port/dbname)
# When using Docker Compose, host should be the service name `db`.
DATABASE_URL=postgres://${POSTGRES_USER}:${POSTGRES_PASSWORD}@db:5432/${POSTGRES_DB}

# Backend node env
NODE_ENV=production
